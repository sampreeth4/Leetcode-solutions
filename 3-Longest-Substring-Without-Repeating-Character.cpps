class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int l=0,r=0,n = s.size(), res=0;
        map<char,int>m;
        while(r < n){
            m[s[r]]++;
            while(m[s[r]] > 1){
                m[s[l]]--;
                l++;
            }
            res = max(res,r-l+1);
            r++;
        }
        return res;
    }
};
